{
  "name": "MyReactNativeApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "yarn react-native start",
    "bundle-ios": "react-native bundle --entry-file index.js --bundle-output ./ios/main.jsbundle --platform ios --assets-dest ./ios --dev false",
    "bundle-android": "react-native bundle --entry-file index.js --bundle-output app/src/main/assets/index.android.bundle --platform android --assets-dest android/app/src/main/res --dev false"
  },
  "dependencies": {
    "@react-native-community/cli": "^4.12.0",
    "metro-react-native-babel-transformer": "0.58.0",
    "react": "16.13.1",
    "react-native": "^0.63.2"
  }
}
